        "metrics": [
          "metric1",
          "metric2"
        ]
      },
      "target": {
        "class": "de.wellnerbou.metrics.send.librato.LibratoSenderFactory",
        "params": {
          "email": "my@email",
          "key": "my-key"
        }
      }
    }
  ]
}
